<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp_pc.aticustestone">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconblack"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityCourts" />
        <activity android:name=".Chatshystory" />
        <activity android:name=".Callshystory" />
        <activity android:name=".Aboutatticus" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".PrimaryNav"
            android:label="@string/title_activity_primary_nav"
            android:theme="@style/AppTheme" />
        <activity android:name=".Chatmenu" />
        <activity android:name=".Testactivity" />
        <activity android:name=".Main2Activity"></activity>
    </application>

</manifest>